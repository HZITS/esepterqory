{
  "entrypoint": "index.html",
  "shell": "src/main-module.html",
  "fragments": [
    "src/dashboard/dashboard-module.html",
    "src/home/home-module.html"
  ],
  "sources": [
    "src/**/*",
    "index.html",
    "src/main-module.html",
    "src/dashboard/dashboard-module.html",
    "src/home/home-module.html"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "builds": [
    {
      "name": "es6",
      "js": {
        "minify": true,
        "compile": false
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": false,
      "addServiceWorker": true,
      "addPushManifest": true,
      "insertPrefetchLinks": true
    }
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  }
}