{
  "entrypoint": "index.html",
  "shell": "src/main-module.html",
  "fragments": [
    "src/topics/topics-module.html",
    "src/sources/sources-module.html",
    "src/articles/articles-module.html"
  ],
  "sources": [
    "src/**/*",
    "index.html",
    "src/main-module.html",
    "src/topics/topics-module.html",
    "src/sources/sources-module.html",
    "src/articles/articles-module.html"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "bower_components/webcomponentsjs/*.js"
  ],
  "builds": [
    {
      "name": "es5",
      "js": {
        "minify": true,
        "compile": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true
    }
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  }
}